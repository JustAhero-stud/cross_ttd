root = true

[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[{*.cs}]
indent_style = tab
indent_size = 4

[{*.axaml}]
indent_style = tab
indent_size = 4

[{*.xml, *.manifest}]
indent_style = tab
indent_size = 2

[{*.json,.*.yml}]
indent_style = space
indent_size = 2
